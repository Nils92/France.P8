pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init()
    p1={
        x=40,
        y=108,
        sprite=1,
        flipx=false
    }
    p2={
        x=80,
        y=108,
        sprite=2,
        flipx=false
    }
    b={
        x=60,
        y=60,
        sprite=3
    }
   	s={
   	y=40,
   	}    
end
function _update60()
if s.y>60 then
s.y-=1
else s.y+=1
end
 if (btn(➡️,0)) then
  p1.flipx=false
  if p1.x<120 then
    p1.x+=1
    end
 end
 if (btn(⬅️,0)) then
  p1.flipx=true
    p1.x-=1
 end
 if (btn(➡️,1)) then
  p2.x+=1 
  p2.flipx=true
 end 
 if (btn(⬅️,1)) then
  p2.x-=1
  p2.flipx=false
 end
  
 
end
function _draw()
    cls()
    map()
 spr(p1.sprite, p1.x, p1.y, 1, 1, p1.flipx)
 spr(p2.sprite, p2.x, p2.y, 1, 1, p2.flipx)
 spr(b.sprite, b.x, b.y, 1, 1)
 spr(10, 70, s.y-10, 1, 1)
 spr(10, 80, s.y-10, 1, 1)
 spr(10, 75, s.y, 1, 1)
 spr(10, 50, s.y, 1, 1)
end
